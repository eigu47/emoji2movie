{
  "name": "emoji2movie",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:next": "next dev --turbopack",
    "dev": "npm-run-all -p db:local:start dev:next",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:local:start": "wsl bash -l -c 'turso dev --db-file local-db.db'",
    "db:local:push": "dotenv -e .env.local -- drizzle-kit push",
    "db:local:studio": "dotenv -e .env.local -- drizzle-kit studio",
    "db:local:seed": "dotenv -e .env.local -- ts-node -r tsconfig-paths/register src/db/seeds/index.ts",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:seed": "dotenv -e .env -- ts-node -r tsconfig-paths/register src/db/seeds/index.ts"
  },
  "dependencies": {
    "@libsql/client": "^0.15.10",
    "@radix-ui/react-dialog": "^1.1.14",
